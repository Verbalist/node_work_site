<script type="text/html" id="editResumeEmployee">
	
		
	<form id="resume_employee_settings_form" class="form-horizontal" autocomplete="on"
              onsubmit="updateResumeEmployee('{{id}}');return false">
		
		<div id="form_element_wrapper">
            <label id="name_label">
            	Name
            </label>

            <input type="text" name="name" id="input_name" value="{{name}}">
        </div>

        <div id="form_element_wrapper">   
            <label id="description_label">
            	Description
            </label>

            <input type="textarea" name="description" id="input_description" value="{{description}}">
        </div>

        <div id="form_element_wrapper">
            <label id="skills_label">
            	Skills
            </label>

            <input type="textarea" name="skills" id="input_skills" value="{{skills}}">
        </div>

        <div id="form_element_wrapper">
            <label id="experience_label">
            	Experience
            </label>

            <input type="textarea" name="experience" id="input_experience" value="{{experience}}">
        </div>

		<div id="form_element_wrapper">
            <label id="education_label">
            	Education
            </label>

            <input type="textarea" name="education" id="input_education" value="{{education}}">
        </div>


		<div id="form_element_wrapper">
            <label id="salary_label">
            	Salary
            </label>

            <input type="textarea" name="salary" id="input_salary" value="{{salary}}">
        </div>

        <div id="submit_resume_edit_wrapper" class="form-group">
             <div class="">
                <button id="submit_resume_edit" name="submit_resume" class="btn btn-info" type="submit">
                    <span class=""></span> Save
                </button>
            </div>
        </div>

	</form>
	    
</script>